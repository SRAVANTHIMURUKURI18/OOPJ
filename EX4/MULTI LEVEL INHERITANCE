class A{
public void methodA(){
System.out.println("method A");
}
}
class B extends A{
public void methodB(){
System.out.println("method B");
}
class C extends B{
public void methodC(){
System.out.println("method C");
}
public static void main(String args[]){
C obj=new C();
obj.methodA();
obj.methodB();
obj.methodC();
}
}

